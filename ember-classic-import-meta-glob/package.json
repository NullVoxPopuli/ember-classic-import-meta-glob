{
  "name": "ember-classic-import-meta-glob",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "files": [
    "addon",
    "babel-plugin.cjs",
    "ember-addon-main.cjs"
  ],
  "scripts": {
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
