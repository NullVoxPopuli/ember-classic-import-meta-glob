  },
  "ember-addon": {
    "version": 1,
    "type": "addon",
    "main": "ember-addon-main.cjs",
    "before": [
      "ember-cli-babel"
    ]
  },
  "dependencies": {
    "@babel/types": "^7.24.6",
    "babel-import-util": "^3.0.0",
    "ember-auto-import": "^2.7.2",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-babel-plugin-helpers": "^1.1.1",
    "picomatch": "^4.0.2"
